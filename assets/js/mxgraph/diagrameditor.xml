				</mxCell>
			</Shape>
		</add>
		<add as="hline">
			<Shape label="" href="">
				<mxCell vertex="1" style="ellipse">		
					<mxGeometry as="geometry" width="60" height="10"/>
				</mxCell>
			</Shape>
		</add>

		<!-- Custom objects -->
		<add as="begin">
			<Roundrect label="Begin" href="">
				<mxCell vertex="1" style="rounded" nameId="1">		
					<mxGeometry as="geometry" width="80" height="40"/>
				</mxCell>
			</Roundrect>
		</add>
		<add as="advancedQuery">
			<Roundrect label="Advanced Query" href="">
				<mxCell vertex="1" style="rounded">		
					<mxGeometry as="geometry" width="100" height="40"/>
				</mxCell>
			</Roundrect>
		</add>
		<add as="databaseInsert">
			<Roundrect label="Database Insert" href="">
				<mxCell vertex="1" style="rounded">		
					<mxGeometry as="geometry" width="100" height="40"/>
				</mxCell>
			</Roundrect>
		</add>
		<add as="databaseDelete">
			<Roundrect label="Database Delete" href="">
				<mxCell vertex="1" style="rounded">		
					<mxGeometry as="geometry" width="100" height="40"/>
				</mxCell>
			</Roundrect>
		</add>
		<add as="databaseQuery">
			<Roundrect label="Database Query" href="">
				<mxCell vertex="1" style="rounded">		
					<mxGeometry as="geometry" width="100" height="40"/>
				</mxCell>
			</Roundrect>
		</add>
		<add as="databaseUpdate">
			<Roundrect label="Database Update" href="">
				<mxCell vertex="1" style="rounded">		
					<mxGeometry as="geometry" width="100" height="40"/>
				</mxCell>
			</Roundrect>
		</add>
		<add as="decision">
			<Roundrect label="Decision" href="">
				<mxCell vertex="1" style="rounded">		
